{
    "name": "deviantintegral/jms-serializer-uri-handler",
    "description": "A handler for serializing and deserializing PSR-7 URI objects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Berry",
            "email": "deviantintegral@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4||^8.0",
        "jms/serializer": "^3.1",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "9.5.26",
        "friendsofphp/php-cs-fixer": "3.13.0"
    },
    "autoload": {
        "psr-4": {
            "Deviantintegral\\JmsSerializerUriHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deviantintegral\\JmsSerializerUriHandler\\Tests\\": "tests/src"
        }
    }
}
